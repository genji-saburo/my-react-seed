{
  "name": "react-example",
  "version": "1.0.0",
  "description": "Simple React example app",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha",
    "watch:test": "mocha --watch --compilers js:babel-register",
    "build": "webpack --env.production",
    "start": "webpack-dev-server --inline --content-base ./ --env.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mzabriskie/react-example"
  },
  "author": "Matt Zabriskie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mzabriskie/react-example/issues"
  },
  "homepage": "https://github.com/mzabriskie/react-example",
  "dependencies": {
    "axios": "0.13.0",
    "bootstrap": "^3.3.5",
    "history": "^3.0.0",
    "moment": "^2.10.6",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-router": "2.5.2",
    "react-tooltip": "3.0.13"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-istanbul": "1.0.3",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.11.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.0",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "file-loader": "0.9.0",
    "jsdom": "9.4.1",
    "lodash": "4.13.1",
    "mocha": "2.5.3",
    "moxios": "0.3.0",
    "nyc": "7.0.0",
    "react-addons-test-utils": "15.2.1",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "starwars-names": "1.6.0",
    "style-loader": "^0.13.0",
    "webpack": "2.1.0-beta.15",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-validator": "2.2.2"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "function": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "exclude": [
      "**/*.(test|stub).js",
      "**/helpers/**",
      "**/fixtures/**"
    ],
    "sourceMap": false,
    "instrument": false,
    "require": [
      "babel-register"
    ]
  }
}
